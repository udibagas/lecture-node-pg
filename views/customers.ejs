<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Customers</title>
  </head>
  <body>
    <h1>Customers</h1>

    <table border="1">
      <thead>
        <tr>
          <th>No.</th>
          <th>Name</th>
        </tr>
      </thead>

      <tbody>
        <% customers.forEach((el, index) => { %>
        <tr>
          <td><%= ++index %></td>
          <td><%= el.name %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
